{
  "name": "ZkDvp",
  "version": "0.5.4",
  "description": "privacy-preserving delivery-versus-payment protocol",
  "license": "MIT",
  "scripts": {
    "lint": "solhint 'contracts/**/*.sol' && eslint .",
    "clean": "npx hardhat clean",
    "circompile": "./scripts/buildCircuits.sh && npx hardhat --network localhost run scripts/initCircuits.js",
    "compile": "npm run clean && npx hardhat compile --force && ./scripts/buildCircuits.sh",
    "deploy": "npx hardhat --network localhost run scripts/deploy.js",
    "init": "npx hardhat --network localhost run scripts/init.js",
    "demo": "npx hardhat --network localhost run scripts/swapDemo.js",
    "demo2": "npx hardhat --network localhost run scripts/erc1155Demo.js",
    "test": "npx hardhat --network localhost test",
    "make": "npm run clean && npx hardhat --network localhost compile && npm run deploy && ./scripts/prepareProject.sh && npm run init",
    "server": "node api/index.js"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.1",
    "@nomiclabs/hardhat-etherscan": "^2.1.3",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "chai": "^4.3.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^7.20.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "ethereum-waffle": "^3.2.2",
    "hardhat": "^2.0.9",
    "hardhat-artifactor": "^0.2.0",
    "hardhat-docgen": "^1.1.1",
    "hardhat-gas-reporter": "^1.0.4",
    "hardhat-tracer": "^1.0.0-alpha.6",
    "maci-crypto": "^2.0.0",
    "solhint": "^3.3.2"
  },
  "dependencies": {
    "@openzeppelin/contracts": "4.1.0",
    "big-integer": "^1.6.52",
    "bignumber.js": "^9.1.2",
    "circomlibjs": "0.0.8",
    "elliptic": "^6.5.7",
    "ethers": "^5.7.2",
    "snarkjs": "^0.7.5",
    "solc": "^0.8.26",
    "web-vitals": "^2.1.4",
    "web3": "^4.12.1",
    "zustand": "^5.0.0-rc.2"
  }
}
